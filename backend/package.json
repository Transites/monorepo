    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.5",
        "@types/supertest": "^6.0.3",
        "@types/bcrypt": "^5.0.1",
        "@types/compression": "^1.7.3",
        "@types/cookie-parser": "^1.4.5",
        "@types/cors": "^2.8.14",
        "@types/jsonwebtoken": "^9.0.4",
        "@types/morgan": "^1.9.6",
        "@types/multer": "^1.4.8",
        "@types/sharp": "^0.32.0",
        "@types/uuid": "^9.0.6",
        "@types/validator": "^13.11.6",
        "@types/pg": "^8.15.4",
        "eslint": "^8.56.0",
        "eslint-config-node": "^4.1.0",
