    "description": "Sistema de submissão e gestão de artigos acadêmicos - Enciclopédia Transitos",
    "main": "server.ts",
    "scripts": {
        "start": "node server.ts",
        "build": "tsc",
        "dev": "nodemon --exec ts-node server.ts",
        "test": "jest --verbose --testPathIgnorePatterns=integration,database",
