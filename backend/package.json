{
  "name": "transitos-backend",
  "version": "1.0.0",
  "description": "Sistema de submissão e gestão de artigos acadêmicos - Enciclopédia Transitos",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --verbose --testPathIgnorePatterns=integration,database",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=integration,database",
    "test:integration": "jest --testPathPattern=integration",
    "seed:admin": "psql $DATABASE_URL -f database/seeds/admin.sql",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "node-cron": "^3.0.3",
    "pg": "^8.14.1",
    "cloudinary": "^1.40.0",
    "resend": "^2.0.0",
    "@supabase/supabase-js": "^2.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-node": "^4.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "nodejs",
    "express",
    "academic",
    "submissions",
    "enciclopedia",
    "transitos",
    "jwt",
    "authentication"
  ],
